#%Module

proc ModulesHelp { } {
    puts stderr "This module sets the path and environment variables for petsc-3.13.0"
    puts stderr "     see https://www.mcs.anl.gov/petsc/ for more information      "
    puts stderr ""
}
module-whatis "PETSc - Portable, Extensible Toolkit for Scientific Computation"

set petsc_dir   "/home/rees/lsa/notebook/venv/lib/python3.8/site-packages/petsc"
set petsc_arch  ""

setenv PETSC_ARCH "$petsc_arch"
setenv PETSC_DIR "$petsc_dir"
prepend-path PATH "/home/rees/lsa/notebook/venv/lib/python3.8/site-packages/petsc/bin"
